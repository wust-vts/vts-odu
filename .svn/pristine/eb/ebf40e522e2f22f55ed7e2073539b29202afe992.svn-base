using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace YimaEncNavigator
{
    [Serializable]
    public class CRadarPackage
    {
        public int radarID { get; set; } //雷达标识
        public int index { get; set; }  //雷达数据索引
        public List<String> lineDataList { get; set; }  //雷达数据包 32条线 512个点的数据

        public CRadarPackage()
        {
            lineDataList = new List<String>();
            index = -1;
            radarID = -1;
        }

        public CRadarPackage(int _radarID, int _index, List<String> lines)
        {
            lineDataList = lines;
            index = _index;
            radarID = _radarID;
        }
    }
}
